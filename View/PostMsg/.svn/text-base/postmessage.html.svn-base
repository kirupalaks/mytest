                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                    <div class="postmessagediv" ng-controller="PostMessageController">
                        <form role="form" name="postMessageForm" class="col-sm-12">
                        <div class="col-lg-9 col-md-6 col-sm-6 nopadding">
                        <div class="form-group col-lg-4">
                        <label>Select Post Type</label>
                        <select class="form-control" name="post_type" id="post_type" required ng-model="mydata.type" 
            					ng-options="obj.type_desc for obj in all_types">
                        	<option></option>
            			</select>
                        </div>
                        
                        <div class="form-group col-lg-4">
                        <label>Select Post Priority</label>
                        <select class="form-control" name="post_priority" id="post_priority" required ng-model="mydata.priority"
        						ng-options="obj.priority for obj in all_priority">
                        	<option></option>
        				</select>
                        </div>

                        <div class="form-group col-lg-4">
                        <label>Link To Test</label>
                        <select class="form-control" name="test_link" id="test_link" required ng-model="mydata.testlink"
            					ng-options="obj.testId for obj in all_testId">
                        	<option></option>
            			</select>
                        </div>
                        </div>
                        
                        <div class="col-lg-3 col-md-6 col-sm-6">
                        <div class="form-group">
                        <label>Post To Batch</label>
                        <select class="form-control" name="post_batches" multiple="multiple" id="post_batches" required integer ng-model="mydata.batches"
        						ng-options="obj.batchId for obj in all_batchId">
        				</select>
                        </div>
                        </div>
                        
                        <div class="form-group col-lg-12 col-md-12 col-sm-12">
                        	<label>Post Description</label>
                        	<textarea class="form-control" rows="3" id="post_description" required ng-model="mydata.desc"></textarea>
                        </div>
                        
                        <div class="date-group col-lg-12 col-md-12 col-sm-12">
                        <div class="form-group col-lg-6 col-md-6 col-sm-6">
                        	<label for="startdate">Start Date</label>
                        	<input class="form-control" name="validateStartdate" id="startdate" validate-startdate type="date" data-role="date" data-inline=true ng-model="mydata.start_date"></input>
                        	<span class="form-error-msg" ng-show="postMessageForm.validateStartdate.$error.todayValidator">Start Date should be current date or later</span>
                		</div>
                		
                		<div class="form-group col-lg-6 col-md-6 col-sm-6">
                			<label for="enddate">Expiry Date</label>
                			<input class="form-control" name="validateEnddate" id="enddate" validate-enddate type="date" data-role="date" data-inline=true ng-model="mydata.exp_date"></input>
                			<span class="form-error-msg" ng-show="postMessageForm.validateEnddate.$error.todayValidator">Expiry Date should be current date or later</span>
                		</div>
                		
                		<span class="form-error-msg" ng-show="start_end_invalid">Start Date should be lesser than Expiry Date</span>
                		</div>
                		
                		<div class="col-md-12 nopadding">
                		<div class="form-group col-lg-6 col-md-6 col-sm-6">
                		<button type="submit" class="btn btn-default" ng-click="postMessage()">Post Message</button>
                		</div>
                		</div>
                		
                        </form>
                        </div> <!-- postmessagediv -->

